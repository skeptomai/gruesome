// Test file to specifically trigger the property access + method call issue

world {
    room test_room "Test Room" {
        desc: "A test room"
        on_look: {
            print("Looking around the test room");
        }
    }
    
    object player {
        names: ["player"]
    }
}

fn test_method_calls() {
    // This should be the problematic line
    player.location.on_look();
    
    // Also test property access
    if player.location.on_look {
        print("Room has on_look method");
    }
}

init {
    player.location = test_room;
    test_method_calls();
}