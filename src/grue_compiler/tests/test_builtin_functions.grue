// Simple test for built-in functions without concatenation
fn main() {
    print("Testing Built-in Functions");
    
    // Test string functions individually without concatenation
    let text = "Hello World";
    
    // Test indexOf
    let index = indexOf(text, "World");
    
    // Test slice
    let sliced = slice(text, 6);
    
    // Test substring  
    let sub = substring(text, 0, 5);
    
    // Test case conversion
    let lower = toLowerCase(text);
    let upper = toUpperCase(text);
    
    // Test trim
    let trimmed = trim("  test  ");
    
    // Test charAt
    let char = charAt(text, 1);
    
    // Test replace
    let replaced = replace(text, "World", "Universe");
    
    // Test boolean functions
    if startsWith(text, "Hello") {
        print("Starts with Hello: true");
    }
    
    if endsWith(text, "World") {
        print("Ends with World: true");
    }
    
    print("Built-in Functions Test Complete");
}