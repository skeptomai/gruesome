{
  "name": "grue",
  "displayName": "Grue Language Support",
  "description": "Syntax highlighting for Grue language",
  "version": "1.0.0",
  "publisher": "vscode",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "grue",
        "aliases": ["Grue", "grue"],
        "extensions": [".grue"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "grue",
        "scopeName": "source.grue",
        "path": "./syntaxes/grue.tmLanguage.json"
      }
    ]
  }
}