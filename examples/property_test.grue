world {
    room start "Test Room" {
        desc: "A room for testing property access."
        
        object box {
            names: ["wooden box", "box"]
            desc: "A wooden box."
            
            contains {
                object key {
                    names: ["brass key", "key"]
                    desc: "A brass key."
                }
            }
        }
    }
}

init {
    print("Testing property access...");
    
    // Test basic property access
    if (box.openable) {
        print("Box is openable");
    }
    
    print("Property access test complete!");
}
