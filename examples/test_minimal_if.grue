world {
    room test_room "Test Room" {
        desc: "A simple test room."
    }
}

grammar {
    verb "test" {
        default => test_comparison()
    }
}

fn test_comparison() {
    print("Before comparison.");
    if (1 == 1) {
        print("Comparison worked.");
    }
    print("After comparison.");
}

init {
    player.location = test_room;
    test_comparison();
}